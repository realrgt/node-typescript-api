    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.23",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^15.6.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "jest": "^27.0.3",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.2",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  }
