{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Typescript node api",
  "main": "main.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/src/main.js",
    "start:dev": "ts-node-dev src/main.ts",
